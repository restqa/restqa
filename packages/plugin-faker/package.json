{
  "name": "@restqa/plugin-faker",
  "main": "src/index.js",
  "description": "RestQA Plugin enabling the generation of fake data to be used in test scenario",
  "version": "0.0.0-generated",
  "keywords": [
    "test automation",
    "restqa",
    "restqa-plugin",
    "testing",
    "cucumber",
    "faker",
    "mock"
  ],
  "scripts": {
    "pretest": "npm run doc",
    "doc": "jsdoc2md  --partial docs/support/scope.hbs --partial docs/support/header.hbs --files src/faker-plugin/steps/**/index.js > docs/steps-catalog.md",
    "lint": "eslint src",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "example": "cucumber-js --require ./example/setup.js --publish-quiet --fail-fast ./example/features"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.10.0",
    "jest-runner-prettier": "^0.3.7",
    "jsdoc-to-markdown": "^6.0.1",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "@restqa/plugin": "0.0.2",
    "faker": "^5.5.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "jest": {
    "projects": [
      {
        "displayName": {
          "name": "test",
          "color": "yellow"
        }
      },
      {
        "runner": "jest-runner-eslint",
        "displayName": "eslint",
        "testMatch": [
          "<rootDir>/src/**/*.js"
        ]
      },
      {
        "runner": "jest-runner-prettier",
        "displayName": {
          "name": "prettier",
          "color": "blue"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restqa/restqa.git"
  },
  "bugs": {
    "url": "https://github.com/restqa/restqa/issues"
  },
  "homepage": "https://github.com/restqa/restqa#readme"
}
