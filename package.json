{
  "name": "@restqa/restqa",
  "version": "1.0.0-rc1",
  "description": "",
  "scripts": {
    "build": "npm run build -ws --if-present",
    "lint": "npm run lint -ws --if-present",
    "format": "npm run format -ws --if-present",
    "example:install": "npm i -w examples/nodejs",
    "example": "npm run happy -w examples/nodejs",
    "test": "npm test -w docs -w cli -w ui --if-present",
    "contribute": "npm install && npm run build && npm run example:install && npm run example",
    "cleanall": "rm pnpm-lock.yaml && rm -rf node_modules && rm -rf packages/*/node_modules"
  },
  "packageManager": "pnpm@7.17.0",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
