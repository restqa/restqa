{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:jest/style",
    "plugin:jest/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-control-regex": 0,
    "no-console": 2
  },
  "globals": {
    "JestQA": true
  }
}

