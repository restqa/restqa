{
  "name": "@restqa/plugin",
  "version": "0.0.4",
  "description": "An abstraction to create RestQA plugin easily",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:e2e": "node test/e2e/test-runner.js",
    "format": "prettier --check index.js test",
    "format:fix": "prettier --write index.js test",
    "lint": "eslint index.js test",
    "lint:fix": "eslint --fix index.js test",
    "check": "npm run format && npm run lint"
  },
  "keywords": [
    "restqa",
    "e2e testing",
    "plugin",
    "cucumber",
    "gherkin",
    "test automation"
  ],
  "homepage": "https://github.com/restqa/plugin",
  "bugs": {
    "url": "https://github.com/restqa/plugin/issues",
    "email": "gorez.tony@gmail.com"
  },
  "author": {
    "name": "Tony Gorez",
    "email": "gorez.tony@gmail.com"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^27.1.0",
    "prettier": "^2.3.2"
  }
}
