{
  "name": "@restqa/data",
  "version": "0.1.1",
  "description": "Data management for test automation - Restqa Labs",
  "main": "index.js",
  "scripts": {
    "serve": "node server.js",
    "serve:dev": "nodemon -r dotenv/config server.js",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "test:coverage": "jest --coverage",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "keywords": [
    "restqa",
    "qa",
    "test automation",
    "data input",
    "quality assurance",
    "confluence",
    "atlassian",
    "google sheets",
    "google sheet",
    "Csv",
    "Express"
  ],
  "author": "Olivier Rodomond <hi@olivierodo.me>",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "csv-parse": "^4.8.8",
    "express": "^4.17.1",
    "got": "^10.7.0",
    "html-table-to-json": "^1.0.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.2",
    "standard": "^14.3.3"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 0
      }
    }
  },
  "standard": {
    "globals": [],
    "env": [
      "jest"
    ],
    "yoda": [
      2,
      "never"
    ]
  }
}
